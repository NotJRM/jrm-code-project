(fs:set-logical-pathname-host
  "SERIAL-IP"
  :physical-host
  (send
    (send (fs:translated-pathname "SYS:SITE;") :host)
    :name)
  :translations
  '(("TESTS;" "DISCUS.TESTS;")
    ("SERIAL-IP;" "DISCUS.SERIAL-IP;")
    ("*;" "DISCUS.SERIAL-IP.*;")
    ("*.*;" "DISCUS.SERIAL-IP.*.*;")))
